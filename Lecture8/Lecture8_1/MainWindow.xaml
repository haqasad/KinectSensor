<Window x:Class="Lecture8_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lecture8_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="700" Loaded="WindowLoaded" Closing="WindowClosing">
    <Grid>
        <Canvas x:Name="skeletonCanvas" HorizontalAlignment="Left" Height="480" Margin="7,10,0,0" VerticalAlignment="Top" Width="640">
            <Ellipse x:Name="righthand" Fill="#FF4D4DEA" Height="26" Canvas.Left="39" Stroke="Black" Canvas.Top="42" Width="27"/>
        </Canvas>
    </Grid>
</Window>
